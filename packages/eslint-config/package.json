{
  "name": "@localazy/eslint-config",
  "version": "1.0.0",
  "description": "Localazy ESLint configuration",
  "keywords": [
    "eslint",
    "eslint-config",
    "localazy"
  ],
  "homepage": "https://github.com/localazy/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localazy/eslint-config.git"
  },
  "license": "MIT",
  "author": {
    "name": "Localazy",
    "email": "team@localazy.com",
    "url": "https://localazy.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.32.0",
    "@typescript-eslint/utils": "^8.38.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-esm-import": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.38.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  }
}
